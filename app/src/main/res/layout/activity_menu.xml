<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Vermelho"
    tools:context=".MenuActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="278dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/identificadorAluno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_logo" />

    <ImageView
        android:id="@+id/calendarioIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="43dp"
        android:layout_marginRight="43dp"
        app:layout_constraintBottom_toBottomOf="@+id/recadosIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/recadosIcon"
        app:layout_constraintTop_toTopOf="@+id/recadosIcon"
        app:srcCompat="@drawable/icon_calendario" />

    <ImageView
        android:id="@+id/agendaIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="41dp"
        android:layout_marginLeft="41dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        app:layout_constraintBottom_toBottomOf="@+id/recadosIcon"
        app:layout_constraintEnd_toStartOf="@+id/recadosIcon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/recadosIcon"
        app:srcCompat="@drawable/icon_agenda" />

    <ImageView
        android:id="@+id/recadosIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="37dp"
        android:layout_marginRight="37dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/calendarioIcon"
        app:layout_constraintStart_toEndOf="@+id/agendaIcon"
        app:layout_constraintTop_toBottomOf="@+id/identificadorAluno"
        app:srcCompat="@drawable/icon_recados" />

    <ImageView
        android:id="@+id/saidaIcon"
        android:layout_width="0dp"
        android:layout_height="105dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="37dp"
        android:layout_marginRight="37dp"
        android:layout_marginBottom="49dp"
        app:layout_constraintBottom_toTopOf="@+id/diarioIcon"
        app:layout_constraintEnd_toStartOf="@+id/segurancaIcon"
        app:layout_constraintStart_toEndOf="@+id/chatIcon"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:srcCompat="@drawable/icon_saida" />

    <ImageView
        android:id="@+id/segurancaIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="43dp"
        android:layout_marginRight="43dp"
        app:layout_constraintBottom_toBottomOf="@+id/saidaIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/saidaIcon"
        app:layout_constraintTop_toTopOf="@+id/saidaIcon"
        app:srcCompat="@drawable/icon_video" />

    <ImageView
        android:id="@+id/financeiroIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="41dp"
        android:layout_marginLeft="41dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        app:layout_constraintBottom_toBottomOf="@+id/diarioIcon"
        app:layout_constraintEnd_toStartOf="@+id/diarioIcon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/diarioIcon"
        app:srcCompat="@drawable/icon_financeiro" />

    <ImageView
        android:id="@+id/diarioIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="37dp"
        android:layout_marginRight="37dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toStartOf="@+id/conteudoIcon"
        app:layout_constraintStart_toEndOf="@+id/financeiroIcon"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:srcCompat="@drawable/icon_professor" />

    <ImageView
        android:id="@+id/conteudoIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="43dp"
        android:layout_marginRight="43dp"
        app:layout_constraintBottom_toBottomOf="@+id/diarioIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/diarioIcon"
        app:layout_constraintTop_toTopOf="@+id/diarioIcon"
        app:srcCompat="@drawable/icon_atividades" />

    <ImageView
        android:id="@+id/chatIcon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="41dp"
        android:layout_marginLeft="41dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        app:layout_constraintBottom_toBottomOf="@+id/saidaIcon"
        app:layout_constraintEnd_toStartOf="@+id/saidaIcon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/saidaIcon"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/icon_chat" />

    <Button
        android:id="@+id/buttonLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="16dp"
        android:text="Log out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView11" />

    <TextView
        android:id="@+id/identificadorAluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="126dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Aluno(a)"
        android:textColor="@color/Amarelo"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:text="Agenda"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/agendaIcon"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/agendaIcon"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Recados"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/recadosIcon"
        app:layout_constraintStart_toStartOf="@+id/recadosIcon"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="301dp"
        android:text="Calendário"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/calendarioIcon"
        app:layout_constraintStart_toStartOf="@+id/calendarioIcon"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Chat"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/chatIcon"
        app:layout_constraintEnd_toEndOf="@+id/chatIcon"
        app:layout_constraintStart_toStartOf="@+id/chatIcon" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Segurança"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/segurancaIcon"
        app:layout_constraintStart_toStartOf="@+id/segurancaIcon"
        app:layout_constraintTop_toBottomOf="@+id/segurancaIcon" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="Financeiro"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/financeiroIcon"
        app:layout_constraintEnd_toEndOf="@+id/financeiroIcon"
        app:layout_constraintStart_toStartOf="@+id/financeiroIcon" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="Diário"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/diarioIcon"
        app:layout_constraintStart_toStartOf="@+id/diarioIcon"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="95dp"
        android:text="Conteúdo"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/conteudoIcon"
        app:layout_constraintStart_toStartOf="@+id/conteudoIcon" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="132dp"
        android:text="Saída"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/saidaIcon"
        app:layout_constraintEnd_toEndOf="@+id/saidaIcon"
        app:layout_constraintStart_toStartOf="@+id/saidaIcon" />

</androidx.constraintlayout.widget.ConstraintLayout>