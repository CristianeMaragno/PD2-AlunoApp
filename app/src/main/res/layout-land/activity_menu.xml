<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Vermelho"
    tools:context=".MenuActivity">

    <ImageView
        android:id="@+id/saidaIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/calendarioIcon"
        app:layout_constraintTop_toTopOf="@+id/chatIcon"
        app:srcCompat="@drawable/icon_saida" />

    <ImageView
        android:id="@+id/calendarioIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toTopOf="@+id/diarioIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/icon_calendario" />

    <ImageView
        android:id="@+id/agendaIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toStartOf="@+id/calendarioIcon"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/recadosIcon"
        app:srcCompat="@drawable/icon_agenda0" />

    <ImageView
        android:id="@+id/recadosIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toStartOf="@+id/calendarioIcon"
        app:layout_constraintStart_toEndOf="@+id/agendaIcon"
        app:layout_constraintTop_toTopOf="@+id/calendarioIcon"
        app:srcCompat="@drawable/icon_recados" />

    <ImageView
        android:id="@+id/financeiroIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toStartOf="@+id/diarioIcon"
        app:layout_constraintStart_toEndOf="@+id/segurancaIcon"
        app:layout_constraintTop_toTopOf="@+id/diarioIcon"
        app:srcCompat="@drawable/icon_financeiro" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Financeiro"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/financeiroIcon"
        app:layout_constraintStart_toStartOf="@+id/financeiroIcon"
        app:layout_constraintTop_toBottomOf="@+id/financeiroIcon" />

    <ImageView
        android:id="@+id/diarioIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/icon_professor" />

    <ImageView
        android:id="@+id/conteudoIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toEndOf="@+id/diarioIcon"
        app:layout_constraintTop_toTopOf="@+id/diarioIcon"
        app:srcCompat="@drawable/icon_atividades" />

    <ImageView
        android:id="@+id/chatIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toStartOf="@+id/saidaIcon"
        app:layout_constraintStart_toEndOf="@+id/calendarioIcon"
        app:layout_constraintTop_toTopOf="@+id/calendarioIcon"
        app:srcCompat="@drawable/icon_chat" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agenda"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/agendaIcon"
        app:layout_constraintStart_toStartOf="@+id/agendaIcon"
        app:layout_constraintTop_toBottomOf="@+id/agendaIcon" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Recados"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/recadosIcon"
        app:layout_constraintStart_toStartOf="@+id/recadosIcon"
        app:layout_constraintTop_toBottomOf="@+id/recadosIcon" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendário"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/calendarioIcon"
        app:layout_constraintStart_toStartOf="@+id/calendarioIcon"
        app:layout_constraintTop_toBottomOf="@+id/calendarioIcon" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chat"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/chatIcon"
        app:layout_constraintStart_toStartOf="@+id/chatIcon"
        app:layout_constraintTop_toBottomOf="@+id/chatIcon" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Segurança"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/segurancaIcon"
        app:layout_constraintStart_toStartOf="@+id/segurancaIcon"
        app:layout_constraintTop_toBottomOf="@+id/segurancaIcon" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diário"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/diarioIcon"
        app:layout_constraintStart_toStartOf="@+id/diarioIcon"
        app:layout_constraintTop_toBottomOf="@+id/diarioIcon" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Saída"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/saidaIcon"
        app:layout_constraintStart_toStartOf="@+id/saidaIcon"
        app:layout_constraintTop_toBottomOf="@+id/saidaIcon" />

    <ImageView
        android:id="@+id/segurancaIcon"
        android:layout_width="@dimen/width_icon_menu"
        android:layout_height="@dimen/width_icon_menu"
        app:layout_constraintEnd_toStartOf="@+id/diarioIcon"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/financeiroIcon"
        app:srcCompat="@drawable/icon_video" />

    <Button
        android:id="@+id/buttonLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="20dp"
        android:text="Log out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/Vermelho"
        android:background="@drawable/button_style2"/>

    <TextView
        android:id="@+id/identificadorAluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:paddingRight="8dp"
        android:paddingBottom="8dp"
        android:text="Aluno(a)"
        android:textColor="@color/Amarelo"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Conteúdo"
        android:textColor="@color/Amarelo"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/conteudoIcon"
        app:layout_constraintStart_toStartOf="@+id/conteudoIcon"
        app:layout_constraintTop_toBottomOf="@+id/conteudoIcon" />

</androidx.constraintlayout.widget.ConstraintLayout>